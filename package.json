{
  "name": "fullstack-demo",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "install": "cd frontend && npm install || true",
    "build": "cd frontend && npm run build",
    "start": "node server/index.js",
    "dev": "concurrently \"npm run start --prefix server\" \"npm run start --prefix frontend\""
  },
  "dependencies": {
    "express": "^4.19.2",
    "http-proxy-middleware": "^3.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  }
} 